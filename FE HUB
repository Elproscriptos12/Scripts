local function QCBNXLM_fake_script() -- Alert.LocalScript 
	local script = Instance.new('LocalScript', Alert)

	local Alert = script.Parent
	while wait(0.05) do
	if Alert.Visible == true then
		Alert.BackgroundColor3 = Color3.new(1, 0.00392157, 0.00392157)
		wait(1)
		Alert.BackgroundColor3 = Color3.new(0.74902, 0.74902, 0.74902)
		wait(1)
		Alert.BackgroundColor3 = Color3.new(1, 0.00392157, 0.00392157)
		wait(1)
		Alert.BackgroundColor3 = Color3.new(0.74902, 0.74902, 0.74902)
		wait(1)
		Alert.BackgroundColor3 = Color3.new(1, 0.00392157, 0.00392157)
		wait(1)
		Alert.BackgroundColor3 = Color3.new(0.74902, 0.74902, 0.74902)
		wait(1)
		Alert.BackgroundColor3 = Color3.new(0.74902, 0.74902, 0.74902)
		wait(1)
			Alert.Visible = false
		end
	end
end
coroutine.wrap(QCBNXLM_fake_script)()
