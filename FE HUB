-- Scripts:

local function WDFUU_fake_script() -- X.LocalScript 
	local script = Instance.new('LocalScript', X)

	local Frame = script.Parent.Parent.Parent
	local Button = script.Parent
	local Text = Frame.TextLabel
	
	Button.MouseButton1Click:Connect(function()
		Button.Visible = false
		Frame.Visible = false
		Text.Visible = false
	end)
end
coroutine.wrap(WDFUU_fake_script)()
local function ZNNPMDJ_fake_script() -- CheckMark.LocalScript 
	local script = Instance.new('LocalScript', CheckMark)

	local Button = script.Parent
	local Troll = Button.Parent.Troll
	local Text = Button.Parent.ToggledFo.Toggled
	local bool = true
	
	Button.MouseButton1Click:Connect(function()
		Button.Visible = false
		Troll.Visible = true
		Text.Text = "Auto Parry"
	end)
	
end
coroutine.wrap(ZNNPMDJ_fake_script)()
local function UXKBZZ_fake_script() -- Effect.LocalScript 
	local script = Instance.new('LocalScript', Effect)

	local Button = script.Parent
	
	Button.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/GamingScripter/Animation-Hub/main/Animation%20Gui", true))()
	
	end)
end
coroutine.wrap(UXKBZZ_fake_script)()
local function AVLGXPL_fake_script() -- Troll.LocalScript 
	local script = Instance.new('LocalScript', Troll)

	local Button = script.Parent
	local TextLabel = script.Parent.Parent.ToggledFo.Toggled
	local Debounce = false
	local Value = true
	local Text = Button.Parent.ToggledFo.Toggled
	local LP = game.Players.LocalPlayer
	local Humanoid = LP.Character:WaitForChild("Humanoid")
	
	Button.MouseButton1Click:Connect(function(bool)
		bool = false
		
		TextLabel.Text = "Auto Parry(toogled)"
		if TextLabel.Text == "Auto Parry(toogled)" then
			Button.Visible = false
			script.Parent.Parent.CheckMark.Visible = true
			bool = true
			if bool == true then
				local function startAutoParry()
					local player = game.Players.LocalPlayer
					local character = player.Character or player.CharacterAdded:Wait()
					local replicatedStorage = game:GetService("ReplicatedStorage")
					local runService = game:GetService("RunService")
					local parryButtonPress = replicatedStorage.Remotes.ParryButtonPress
					local ballsFolder = workspace:WaitForChild("Balls")
	
					print("Script successfully ran.")
	
					local function onCharacterAdded(newCharacter)
						character = newCharacter
					end
	
					player.CharacterAdded:Connect(onCharacterAdded)
	
					local focusedBall = nil  
	
					local function chooseNewFocusedBall()
						local balls = ballsFolder:GetChildren()
						focusedBall = nil
						for _, ball in ipairs(balls) do
							if ball:GetAttribute("realBall") == true then
								focusedBall = ball
								break
							end
						end
					end
	
					chooseNewFocusedBall()
	
					local function timeUntilImpact(ballVelocity, distanceToPlayer, playerVelocity)
						local directionToPlayer = (character.HumanoidRootPart.Position - focusedBall.Position).Unit
						local velocityTowardsPlayer = ballVelocity:Dot(directionToPlayer) - playerVelocity:Dot(directionToPlayer)
	
						if velocityTowardsPlayer <= 0 then
							return math.huge
						end
	
						local distanceToBeCovered = distanceToPlayer - 40
						return distanceToBeCovered / velocityTowardsPlayer
					end
	
					local BASE_THRESHOLD = 0.15
					local VELOCITY_SCALING_FACTOR = 0.002
	
					local function getDynamicThreshold(ballVelocityMagnitude)
						local adjustedThreshold = BASE_THRESHOLD - (ballVelocityMagnitude * VELOCITY_SCALING_FACTOR)
						return math.max(0.12, adjustedThreshold)
					end
	
					local function checkBallDistance()
						if not character:FindFirstChild("Highlight") then return end
						local charPos = character.PrimaryPart.Position
						local charVel = character.PrimaryPart.Velocity
	
						if focusedBall and not focusedBall.Parent then
							chooseNewFocusedBall()
						end
	
						if not focusedBall then return end
	
						local ball = focusedBall
						local distanceToPlayer = (ball.Position - charPos).Magnitude
	
						if distanceToPlayer < 10 then
							parryButtonPress:Fire()
							return
						end
	
						local timeToImpact = timeUntilImpact(ball.Velocity, distanceToPlayer, charVel)
						local dynamicThreshold = getDynamicThreshold(ball.Velocity.Magnitude)
	
						if timeToImpact < dynamicThreshold then
							parryButtonPress:Fire()
						end
					end
					heartbeatConnection = game:GetService("RunService").Heartbeat:Connect(function()
						checkBallDistance()
					end)
				end
				startAutoParry()
				
				local function stopAutoParry()
					if heartbeatConnection then
						heartbeatConnection:Disconnect()
						heartbeatConnection = nil
					end
				end
				if TextLabel.Text == "Auto Parry" then
					stopAutoParry()
				end
		end
		end
	end)
		
	
	
end
coroutine.wrap(AVLGXPL_fake_script)()
