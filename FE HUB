local function WIGK_fake_script() -- Open.LocalScript 
	local script = Instance.new('LocalScript', Open)

	local Button = script.Parent
	local Frame = script.Parent.Parent.Frame
	local Frame2 = script.Parent.Parent.Swords
	local X2 = Frame2.TextLabel.X
	local Text2 = Frame2.TextLabel
	local X = script.Parent.Parent.Frame.TextLabel.X
	local Text = script.Parent.Parent.Frame.TextLabel
	
	
	
		
	
	Button.MouseButton1Click:Connect(function()
		Frame.Visible = true
		X.Visible = true
		Text.Visible = true
		X2.Visible = true
		Text2.Visible = true
		end)
	
	
end
coroutine.wrap(WIGK_fake_script)()
